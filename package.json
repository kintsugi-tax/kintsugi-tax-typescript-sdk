{
  "name": "@kintsugi-tax/tax-platform-sdk",
  "version": "0.4.15",
  "author": "Kintsugi Tax",
  "type": "module",
  "tshy": {
    "sourceDialects": [
      "@kintsugi-tax/tax-platform-sdk/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./types": "./src/types/index.ts",
      "./models/errors": "./src/models/errors/index.ts",
      "./models": "./src/models/index.ts",
      "./models/operations": "./src/models/operations/index.ts",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kintsugi-tax/kintsugi-speakeasy.git",
    "directory": "sdks/typescript"
  },
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "build": "tshy",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "zod": "^3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "tshy": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.22.0",
    "zod": "^3.25.17"
  },
  "dependencies": {
    
  }
}
